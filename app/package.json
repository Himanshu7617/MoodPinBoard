{
  "name": "moodboard",
  "private": true,
  "version": "0.0.0",
  "description": "MoodBoard desktop application",
  "type": "module",
  "author": "MoodBoard Developers",
  "main": "dist-electron/main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/Himanshu7617/MoodPinBoard.git"
  },
  "scripts": {
    "dev:react": "vite",
    "dev:electron": "cross-env PINTEREST_CLIENT_ID=1534395 REDIRECT_URI=http://localhost:3333/oauth2callback  NODE_ENV=development electron .",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "transpile:electron": "tsc --project src/electron/tsconfig.json",
    "dist:mac": "cross-env PINTEREST_CLIENT_ID=1534395 REDIRECT_URI=http://localhost:3333/oauth2callback npm run transpile:electron && npm run build && electron-builder --mac --arm64",
    "dist:win": "cross-env PINTEREST_CLIENT_ID=1534395 REDIRECT_URI=http://localhost:3333/oauth2callback npm run transpile:electron && npm run build && electron-builder --win --x64",
    "dist:linux": "cross-env PINTEREST_CLIENT_ID=1534395 REDIRECT_URI=http://localhost:3333/oauth2callback npm run transpile:electron && npm run build && electron-builder --linux --x64",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish"
  },
  "dependencies": {
    "axios": "^1.12.2",
    "dotenv": "^17.2.3",
    "electron-squirrel-startup": "^1.0.1",
    "electron-store": "^11.0.2",
    "get-port": "^7.1.0",
    "googleapis": "^164.0.0",
    "html-to-image": "^1.11.13",
    "http": "^0.0.1-security",
    "open": "^10.2.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.9.6",
    "update-electron-app": "^3.1.1"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.10.2",
    "@electron-forge/maker-deb": "^7.10.2",
    "@electron-forge/maker-rpm": "^7.10.2",
    "@electron-forge/maker-squirrel": "^7.10.2",
    "@electron-forge/maker-zip": "^7.10.2",
    "@electron-forge/plugin-auto-unpack-natives": "^7.10.2",
    "@electron-forge/plugin-fuses": "^7.10.2",
    "@electron-forge/publisher-github": "^7.10.2",
    "@electron/fuses": "^1.8.0",
    "@eslint/js": "^9.25.0",
    "@tailwindcss/vite": "^4.1.16",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "cross-env": "^10.1.0",
    "electron": "^38.3.0",
    "electron-builder": "^26.0.12",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "npm-run-all": "^4.1.5",
    "tailwindcss": "^4.1.16",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5",
    "vitest": "^3.2.4"
  }
}
